<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="param_partner_check_address" model="ir.config_parameter">
        <field name="key">partner_check.address_fields</field>
        <field name="value">street,zip,city</field>
    </record>

    <record id="param_partner_check_account_out" model="ir.config_parameter">
        <field name="key">partner_check.account_fields.out</field>
        <field name="value">debitor_number</field>
    </record>

    <record id="param_partner_check_account_in" model="ir.config_parameter">
        <field name="key">partner_check.account_fields.in</field>
        <field name="value">creditor_number</field>
    </record>
</odoo>
